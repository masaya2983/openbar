<%= form_with model: bar, local:true do |f|%>
<div class="form-grup">
  <%= f.label :title ,"タイトル"%>
  <%= f.text_field :title, class: 'form-control liquor_name' %>
</div>
<div class="form-group">
  <%= f.label:body,"レビュー"%>
  <%= f.text_area :body %>
</div>
<div class="form-group">
  <%= f.submit class: 'btn btn-success' %>
  </div>
<% end %>